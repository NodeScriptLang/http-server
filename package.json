{
    "name": "@nodescript/http-server",
    "version": "0.4.3",
    "description": "Standard HTTP Server",
    "type": "module",
    "sideEffects": false,
    "exports": {
        ".": "./out/main/index.js"
    },
    "main": "out/main/index.js",
    "types": "out/main/index.d.ts",
    "files": [
        "out/main/**/*"
    ],
    "scripts": {
        "clean": "rm -rf out *.tsbuildinfo",
        "dev": "tsc -b -w",
        "compile": "tsc -b",
        "lint": "eslint --ext=.js,.ts,.vue --cache .",
        "test": "NODE_ENV=test mocha",
        "version": "npm run compile",
        "postversion": "npm publish --access=public && git push --tags origin main"
    },
    "pre-commit": [
        "lint"
    ],
    "repository": {
        "type": "git",
        "url": "git+ssh://git@github.com/nodescript/http-server.git"
    },
    "keywords": [],
    "author": "Boris Okunskiy",
    "license": "ISC",
    "devDependencies": {
        "@nodescript/eslint-config": "^1.0.0",
        "@types/mocha": "^8.2.3",
        "@types/node": "^16.3.1",
        "eslint": "^8.24.0",
        "mocha": "^9.0.2",
        "pre-commit": "^1.2.2",
        "typescript": "^4.3.5"
    },
    "dependencies": {
        "@nodescript/config": "^1.0.6",
        "@nodescript/errors": "^1.0.1",
        "@nodescript/exception": "^2.0.6",
        "@nodescript/logger": "^1.4.0",
        "@nodescript/mesh": "^3.1.2",
        "@nodescript/protocomm": "^0.5.0",
        "@koa/cors": "^3.4.1",
        "@types/koa": "^2.13.5",
        "@types/koa__cors": "^3.3.0",
        "@types/stoppable": "^1.1.1",
        "koa": "^2.13.4",
        "koa-body": "^5.0.0",
        "stoppable": "^1.1.0"
    }
}
