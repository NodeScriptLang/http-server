{
    "name": "@nodescript/http-server",
    "version": "1.0.12",
    "description": "Standard HTTP Server",
    "type": "module",
    "sideEffects": false,
    "exports": {
        ".": "./out/main/index.js"
    },
    "main": "out/main/index.js",
    "types": "out/main/index.d.ts",
    "files": [
        "out/main/**/*"
    ],
    "scripts": {
        "clean": "rm -rf out *.tsbuildinfo",
        "dev": "npm run clean && tsc -b -w",
        "compile": "tsc -b",
        "lint": "eslint --ext=.js,.ts,.vue --cache .",
        "test": "NODE_NO_WARNINGS=1 NODE_ENV=test mocha ",
        "version": "npm run compile",
        "postversion": "npm publish --access=public && git push --tags origin main"
    },
    "pre-commit": [
        "lint"
    ],
    "repository": {
        "type": "git",
        "url": "git+ssh://git@github.com/nodescript/http-server.git"
    },
    "keywords": [],
    "author": "Boris Okunskiy",
    "license": "ISC",
    "devDependencies": {
        "@nodescript/eslint-config": "^1.0.0",
        "@types/mocha": "^8.2.3",
        "@types/node": "^18.13.0",
        "dotenv": "^16.0.3",
        "eslint": "^8.24.0",
        "mocha": "^9.0.2",
        "pre-commit": "^1.2.2",
        "typescript": "^4.3.5"
    },
    "dependencies": {
        "@nodescript/errors": "^1.1.1",
        "@nodescript/logger": "^1.4.1",
        "@nodescript/metrics": "^1.1.0",
        "@nodescript/pathmatcher": "^1.2.0",
        "@nodescript/protocomm": "^0.5.5",
        "mesh-config": "^1.0.0",
        "mesh-ioc": "^3.1.4",
        "nanoevent": "^1.0.0"
    }
}
